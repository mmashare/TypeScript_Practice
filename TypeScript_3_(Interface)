///////////////////////////// importance of interfaces in ts //////////////////////////

// const ufcFighter ={
//     name:"khabib",
//     id:0
// }
/// it saves to from getting error{Object literal may only specify known properties} because in interface we have decide that the obj key should be 
// that type(that you give it)
//// one thing that is always know that never give other key that is'nt present in you interfaces, if you do that then is gives you an error

/// you can say that interface is a type of class

interface Fighter {
    name: string,
    id:number
}

/// and these are the object of that class(interface that you make)

const upFighter:Fighter= {
    name:"himnashu",
    id:9
}
console.log(upFighter)

const delhiFighter:Fighter ={
    name:"inshul",
    id:9
}
const mumbaiFighter:Fighter ={
    name:"anskush",
    id:8
}
